{
    "name": "andrewcarteruk/nomoreleaksbundle",
    "description": "A Symfony bundle that kills memory leaks using lasers and ninja stars",
    "keywords": ["nomoreleaks", "memory", "leaks", "bundle", "no", "more"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Carter",
            "email": "andrewcarter1992@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/yaml": "~2.7|~3.0"
    },
    "autoload": {
        "psr-4": {
            "AndrewCarterUK\\NoMoreLeaksBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndrewCarterUK\\NoMoreLeaksBundle\\Tests\\": "tests/"
        }
    }
}
